{
  "manifest_version": 2,

  "name": "To-do List App",
  "description": "This extension shows keeps track of your daily tasks",
  "version": "1.0",



  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Open to-do list"
  },

  "permissions": [
    "tabs"
  ]
}
